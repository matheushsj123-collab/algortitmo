    escolha(seleciona){
      caso 1 :{
        escreva("Fogo")
        pare
        }
         caso 2 :{
        escreva("Água")
        pare
        }
         caso 3 :{
        escreva("Terra")
        pare
        }
         caso 4 :{
        escreva("Mar")
        pare
        }
        caso contrario :
        escreva("Voçê caiu na armadilha , corra !!")
   
   
    }

  }
}
